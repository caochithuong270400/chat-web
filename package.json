{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev:nuxt": "nuxt dev --host --port=5000",
    "dev:server": "nodemon",
    "dev": "concurrently -n \"NUXT,SERVER\" \"yarn dev:nuxt\" \"yarn dev:server\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "pm2 start .output/server/index.mjs --name my-nuxt-app"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/nitro": "^0.10.0",
    "@pinia/nuxt": "^0.5.1",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "nodemon": "^3.1.7",
    "numeral": "^2.0.6",
    "nuxt": "^3.12.2",
    "sass": "^1.77.6",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "vue": "latest",
    "vuetify": "^3.6.10"
  }
}
